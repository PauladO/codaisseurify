<h1>All artists</h1>
<hr>
<% @artists.each do |artist| %>
  <%= image_tag artist.image %>
  <h3><%= link_to artist.name, artist_path(artist.id) %></h3>
  <%= button_to "delete artist", {:action => 'destroy', :id => artist.id }, :method => :delete, data: {confirm: "Are you sure you want to delete #{artist.name}?"}
 %>

<% end %>
